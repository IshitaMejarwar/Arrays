#include <iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    int a[n];
    int maximumnum=0;
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    for(int i=0;i<n;i++){
         int currentnum=a[i];
        if(currentnum>maximumnum){
           maximumnum=currentnum;
        }
    }
    cout<<maximumnum;
    return 0;
}
